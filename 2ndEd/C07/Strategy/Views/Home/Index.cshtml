@model HomePageViewModel
@{
    ViewData["Title"] = "Home Page";
}
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    @if(Model != null)
    {
        <p>Here are your data:</p>
        <ul class="list-group">
        @foreach (var item in Model.SomeData)
        {
            <li class="list-group-item">@item</li>
        }
        </ul>
    }
</div>
